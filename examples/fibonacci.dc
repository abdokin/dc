fn main(int argc,[]string argv)  int {
    print("Calculating fibonacci!\n");
    let x:int = 10;
    let n:int = fib(x);
    print("feb({x}) =  {n}\n");
    return 0;
}

fn feb(int n)  int {
    if n <= 0 {
        return 0;
    }else if n == 1 {
        return 1;
    }else {
        return feb(n -1) + feb(n+1)
    }
    return 0;
}
